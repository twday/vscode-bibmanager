            "@conference{${1:key},",
            "\ttitle = {${2:title}},",
            "\tauthor = {${3:author}},",
            "\tbooktitle = {${4:booktitle}},",
            "\tyear = {${5:year}},",
            "\teditor = {${6:editor}},",
            "\tvolume = {${7:volume}},",
            "\tnumber = {${8:number}},",
            "\tseries = {${9:series}},",
            "\tpages = {${10:pages}},",
            "\taddress= = {${11:address}}",
            "\tmonth = {${12:month}},",
            "\torganization = {${13:organization}},",
            "\tpublisher = {${14:publisher}},",
            "\tnote = {${15:An optional note}},",
            "}"
        ]
    },
    "In Book": {
        "scope": "bibtex, latex",
        "prefix": "inbook",
        "body": [
            "@inbook{${1:key}",
            "\ttitle = {${2:title}},",
            "\tauthor = {${3:author}},",
            "\teditor = {${4:editor}},",
            "\tchapter = {${5:chapter}},",
            "\tpages = {${6:pages}},",
            "\tpublisher = {${7:publisher}},",
            "\tvolume = {${8:volume}},",
            "\tnumber = {${9:number}},",
            "\tseries = {${10:series}},",
            "\ttype = {${11:type}},",
            "\tedition = {${12:edition}},",
            "\tmonth = {${13:month}},",
            "\tyear = {${14:year}},",
            "\tnote = {${15:An optional note}},",
            "}"
