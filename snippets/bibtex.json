{
    "Article Reference": {
        "scope": "bibtex, latex",
        "prefix": "article",
        "body": [
            "@article{${1:key},",
            "\ttitle = {${2:title}},",
            "\tauthor = {${3:author}},",
            "\tjournal = {${4:journal}},",
            "\tpages = {${5:pages}},",
            "\tnumber = {${6:number},",
            "\tvolume = {${7:volume}},",
            "\tmonth = {${8:month}},",
            "\tyear = {${9:year}},",
            "\tnote = {${10:An optional note}},",
            "}"
        ]
    },
    "Book Reference": {
        "scope": "bibtex, latex",
        "prefix": "book",
        "body": [
            "@book{${1:key},",
            "\ttitle = {${2:title}},",
            "\tauthor = {${3:author}},",
            "\teditor = {${4:editor}},",
            "\tpublisher = {${5:publisher}},",
            "\tvolume = {${6:volume}},",
            "\tnumber = {${7:number},",
            "\tseries = {${8:series}},",
            "\tedition = {${9:edition}},",
            "\tmonth {${10:month}},",
            "\tyear = {${11:year}}",
            "\taddress= = {${12:address}}",
            "\tnote = {${13:An optional note},",
            "}"
        ]
    },
    "Booklet Reference": {
        "scope": "bibtex, latex",
        "prefix": "booklet",
        "body":[
            "@booklet{${1:key},",
            "\ttitle = {${2:title}},",
            "\tauthor = {${3:author}},",
            "\thowpublished = {${4:howpublished},",
            "\tmonth = {${5:month}},",
            "\tyear = {${6:year}},",
            "\taddress = {${7:address}},",
            "\tnote = {${8:An optional note}},",
            "}"
        ]
    },
    "Conference Reference": {
        "scope": "bibtex, latex",
        "prefix": "conference",
