{
    "Article Reference": {
        "scope": "bibtex, latex",
        "prefix": "article",
        "body": [
            "@article{${1:key},",
            "\ttitle = {${2:title}},",
            "\tauthor = {${3:author}},",
            "\tjournal = {${4:journal}},",
            "\tpages = {${5:pages}},",
            "\tnumber = {${6:number},",
            "\tvolume = {${7:volume}},",
            "\tmonth = {${8:month}},",
            "\tyear = {${9:year}},",
            "\tnote = {${10:An optional note}},",
            "}"
        ]
    },
    "Booklet Reference": {
        "scope": "bibtex, latex",
        "prefix": "booklet",
        "body":[
            "@booklet{${1:key},",
            "\ttitle = {${2:title}},",
            "\tauthor = {${3:author}},",
            "\thowpublished = {${4},",
            "\tmonth = {${5:month}},",
            "\tyear = {${6:year}},",
            "\tnote = {${7:An optional note}},",
            "}"
        ]
    },
    "Conference Reference": {
        "scope": "bibtex, latex",
        "prefix": "conference",
        "body": [
            "@conference{${1:key},",
            "\ttitle = {${2:title}},",
            "\tauthor = {${3:author}},",
            "\tbooktitle = {${4:booktitle}},",
            "\tyear = {${5:year}},",
            "\teditor = {${6:editor}},",
            "\tvolume = {${7:volume}},",
            "\tseries = {${8:series}},",
            "\tpages = {${9:pages}},",
            "\tmonth = {${10:month}},",
            "\torganization = {${11:organization}},",
            "\tpublisher = {${12:publisher}},",
            "\tnote = {${13:An optional note}},",
            "}"
        ]
    },
    "In Book": {
        "scope": "bibtex, latex",
        "prefix": "inbook",
        "body": [
            "@inbook{${1:key}",
            "\ttitle = {${2:title}},",
            "\tauthor = {${3:author}},",
            "\tchapter = {${4:chapter}},",
            "\tpages = {${5:pages}},",
            "\tpublisher = {${6:publisher}},",
            "\tvolume = {${8:volume}},",
            "\tseries = {${9:series}},",
            "\tedition = {${10:edition}},",
            "\tmonth = {${11:month}},",
            "\tyear = {${7:year}},",
            "\tnote = {${12:An optional note}},",
            "}"
        ]
    },
    "In Collection": {
        "scope": "bibtex, latex",
        "prefix": "incollection",
        "body": [
            "@incollection{${1:key},",
            "\ttitle = {${2:title}},",
            "\tauthor = {${3:author}},",
            "\tbooktitle = {${4:booktitle}},",
            "\tpublisher = {${5:publisher}},",
            "\tyear = {${6:year}},",
            "\teditor = {${7:editor}},",
            "\tvolume = {${8:volume}},",
            "\tseries = {${9:series}},",
            "\tchapter = {${10:chapter}},",
            "\tpages = {${11:pages}},",
            "\tedition = {${12:edition}},",
            "\tmonth = {${13:month}},",
            "\tnote = {${14:An optional note}},",
            "}"
        ]
    },
    "Book Reference": {
        "scope": "bibtex, latex",
        "prefix": "book",
        "body": [
            "@book{${1:key},",
            "\ttitle = {${2:title}},",
            "\tauthor = {${3:author}},",
            "\tjournal = {${4:journal}},",
            "\tpublisher = {${5:publisher}},",
            "\tvolume = {${6:volume}},",
            "\tseries = {${7:series}},",
            "\tedition = {${8:edition}},",
            "\tmonth {${9:month}},",
            "\tyear = {${10:year}}",
            "\tisbn = {${11:isbn}},",
            "\tnote = {${12:An optional note},",
            "}"
        ]
    },
    "Reference in proceedings": {
        "scope": "bibtex, latex",
        "prefix": "inproceedings",
        "body": [
            "@inproceedings{${1:key},",
            "\ttitle = {${2:title}},",
            "\tauthor = {${3:author}},",
            "\tjournal = {${4:journal}},",
            "\tyear = {${5:year}}",
            "\teditor = {${6:editor}},",
            "\tvolume = {${7:volume}},",
            "\tseries = {${8:series}},",
            "\tmonth = {${9:month}},",
            "\torganization = {${10:organization}},",
            "\tpublisher = {${11:publisher}},",
            "\tnote = {${12:An optional note}},",
            "}"
        ]
    },
    "Electronic Reference": {
        "scope": "bibtex, latex",
        "prefix": "electronic",
        "body": [
            "@electronic{${1:key},",
            "\ttitle = {${2:title}},",
            "\tauthor = {${3:author}},",
            "\tjournal = {${4:journal}},",
            "\tyear = {${5:year}}",
            "}"
        ]
    },
    "Miscellaneous Reference": {
        "scope": "bibtex, latex",
        "prefix": "misc",
        "body": [
            "@misc{${1:key},",
            "\ttitle = {${2:title}},",
            "\thowpublished = {$3},",
            "\tmonth = {${4:month}},",
            "\tyear = {${5:year}},",
            "\tnote = {$6},",
            "}"

        ]
    },
    "Title of the Paper":{
        "scope": "bibtex, latex",
        "prefix": "title",
        "body":[
            "title = {${1:title}},"
        ]
    },
    "List of Authors":{
        "scope": "bibtex, latex",
        "prefix": "authors",
        "body":[
            "author = {${1:author}},"
        ]
    },
    "Reference Journal":{
        "scope": "bibtex, latex",
        "prefix": "journal",
        "body": [
            "journal = {${1:journal}},"
        ]
    },
    "Pages of References":{
        "scope": "bibtex, latex",
        "prefix": "pages",
        "body": [
            "pages = {${1}-${2}}"
        ]
    },
    "Year of the Publication":{
        "scope": "bibtex, latex",
        "prefix": "year",
        "body":[
            "year = {${1:year}}"
        ]
    }
}
