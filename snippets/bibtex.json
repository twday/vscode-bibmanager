            "\tnote = {${15:An optional note}},",
            "}"
        ]
    },
    "Miscellaneous Reference": {
        "scope": "bibtex, latex",
        "prefix": "misc",
        "body": [
            "@misc{${1:key},",
            "\ttitle = {${2:title}},",
            "\thowpublished = {$3},",
            "\tmonth = {${4:month}},",
            "\tyear = {${5:year}},",
            "\tnote = {$6},",
            "}"
        ]
    }
